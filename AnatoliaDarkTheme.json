  "tokenColors": [
    {
      "scope": [
        "source"
      ],
      "settings": {
        "foreground": "#eaf2f1"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#888d94",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffd76d"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ff9b5e"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#9cd1bb"
      }
    },
    {
      "scope": [
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#ffd76d"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.other",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9cd1bb"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#ff9b5e"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "storage.type",
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.logical",
        "keyword.other",
        "keyword.control",
        "entity.name.tag",
        "storage",
        "storage.type.function.arrow",
        "storage.type.function",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#9CBBDB"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#bbdb9c"
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#9CBBDB"
      }
    },
    {
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#ffd76d"
      }
    }
  ]
