    "tokenColors": [
      {
        "scope":[
          "variable",
          "source"
        ],
        "settings": {
          "foreground": "#333333"
        }
      },
      {
        "scope": "comment",
        "settings": {
          "foreground": "#888d94",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "string",
        "settings": {
          "foreground": "#d68c00"
        }
      },
      {
        "scope": "keyword",
        "settings": {
          "foreground": "#e67e22"
        }
      },
      {
        "scope": ["entity.name.class"],
        "settings": {
          "foreground": "#d68c00"
        }
      },
      {
        "scope": [
          "constant.numeric",
          "constant.character",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.other",
          "entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#26914c"
        }
      },
      {
        "scope": [
          "constant.language",
          "constant.language.boolean"
        ],
        "settings": {
          "foreground": "#e67e22"
        }
      },
      {
        "scope": [
          "storage.modifier",
          "storage.type",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.arithmetic",
          "keyword.operator.assignment",
          "keyword.operator.logical",
          "keyword.other",
          "keyword.control",
          "entity.name.tag",
          "storage",
          "storage.type.function.arrow",
          "storage.type.function",
          "support.class.component.tsx"
        ],
        "settings": {
          "foreground": "#007acc"
        }
      },
      {
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#26914c"
        }
      },
      {
        "scope": "support.type.property-name.json",
        "settings": {
          "foreground": "#007acc"
        }
      },
      {
        "scope": "meta.structure.dictionary.json string.quoted.double.json",
        "settings": {
          "foreground": "#d68c00"
        }
      }
    ]
  }
  